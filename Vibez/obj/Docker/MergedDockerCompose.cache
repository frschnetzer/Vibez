name: dockercompose15026841806319855851
services:
  db:
    container_name: mssql-container
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: admin1234!
    image: mcr.microsoft.com/mssql/server
    networks:
      mynetwork: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: volume
        source: mssql-data
        target: /var/opt/mssql
        volume: {}
  vibez:
    build:
      context: C:\Users\Schnetzer Franziska\Source\Repos\frschnetzer\Vibez\Vibez
      dockerfile: Vibez/Dockerfile
    depends_on:
      db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: vibez
    networks:
      mynetwork: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Schnetzer Franziska\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Schnetzer Franziska\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  mynetwork:
    name: dockercompose15026841806319855851_mynetwork
volumes:
  mssql-data:
    name: dockercompose15026841806319855851_mssql-data